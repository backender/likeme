{% extends "::base.html.twig" %}

{% block body %}

        {% for key, message in app.session.getFlashes() %}
        <div class="{{ key }}">
            {{ message|trans({}, 'FOSUserBundle') }}
        </div>
        {% endfor %}

        <div id="content">
            {% block fos_user_content %}{% block title %}{% endblock %}{% endblock fos_user_content %}
            
            {%block sider %}
				{% include 'LikemeSystemBundle:Sider:sider.html.twig' %}
			{%endblock sider %}
        </div>

{% endblock body %}